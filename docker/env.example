# =============================================================================
# E2I Causal Analytics - Environment Variables
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# =============================================================================

# -----------------------------------------------------------------------------
# SUPABASE (Cloud)
# -----------------------------------------------------------------------------
# Get these from your Supabase project settings
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# -----------------------------------------------------------------------------
# ANTHROPIC (Claude API)
# -----------------------------------------------------------------------------
ANTHROPIC_API_KEY=sk-ant-your-api-key

# -----------------------------------------------------------------------------
# APPLICATION
# -----------------------------------------------------------------------------
ENVIRONMENT=development
LOG_LEVEL=DEBUG

# -----------------------------------------------------------------------------
# MEMORY SYSTEMS (Auto-configured by Docker Compose)
# -----------------------------------------------------------------------------
# These are set automatically but can be overridden for external services
# Port mapping (per docker/docker-compose.yml):
#   - e2i_redis (Working Memory): 6382
#   - e2i_falkordb (Semantic Memory): 6381
#   - opik-redis (Opik, separate): 6390
#   - auto-claude-falkordb (external): 6380
# REDIS_URL=redis://localhost:6382/0
# FALKORDB_URL=redis://localhost:6381/0

# -----------------------------------------------------------------------------
# MLOPS SERVICES (Auto-configured by Docker Compose)
# -----------------------------------------------------------------------------
# MLFLOW_TRACKING_URI=http://localhost:5000
# BENTOML_URL=http://localhost:3000
# FEAST_URL=http://localhost:6566
# OPIK_URL=http://localhost:5174

# -----------------------------------------------------------------------------
# CELERY (Auto-configured by Docker Compose)
# -----------------------------------------------------------------------------
# CELERY_BROKER_URL=redis://localhost:6382/1
# CELERY_RESULT_BACKEND=redis://localhost:6382/2

# -----------------------------------------------------------------------------
# DEVELOPMENT TOOLS
# -----------------------------------------------------------------------------
# Flower (Celery monitoring) credentials
FLOWER_USER=admin
FLOWER_PASSWORD=admin

# VS Code debugpy
DEBUGPY_ENABLE=true
DEBUGPY_PORT=5678

# -----------------------------------------------------------------------------
# FRONTEND
# -----------------------------------------------------------------------------
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000
